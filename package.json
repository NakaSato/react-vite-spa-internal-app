{
  "name": "react-vite-spa",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "bun x vite --host",
    "dev:fast": "FORCE_OPTIMIZE=false bun x vite --host --force",
    "dev:turbo": "FORCE_OPTIMIZE=false bun x vite --host --force --no-deps",
    "dev:prod": "bun x vite --host",
    "build:dev": "bun x vite build",
    "build:vite": "bun x vite build",
    "build:analyze": "ANALYZE=true bun x vite build",
    "build:fast": "bun x vite build --minify false",
    "preview": "bun x vite preview",
    "preview:port": "bun x vite preview --port 4173",
    "clean": "rm -rf dist node_modules/.vite node_modules/.vite-dev .vite",
    "clean:cache": "rm -rf node_modules/.vite node_modules/.vite-dev .vite",
    "start": "bun dev",
    "build": "bun x vite build",
    "start:prod": "bun preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@react-pdf/renderer": "^4.3.0",
    "@react-spring/web": "^10.0.1",
    "@types/d3": "^7.4.3",
    "@types/socket.io-client": "^3.0.0",
    "buffer": "^6.0.3",
    "chart.js": "^4.5.0",
    "d3": "^7.9.0",
    "framer-motion": "^12.23.12",
    "immer": "^10.1.1",
    "lottie-react": "^2.4.1",
    "pdf-parse": "^1.1.1",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^7.7.1",
    "react-virtualized": "^9.22.6",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^3.4.17",
    "tesseract.js": "^6.0.1",
    "workbox-core": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0",
    "workbox-webpack-plugin": "^7.3.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "^4.9.5",
    "vite": "^7.0.6",
    "vite-plugin-html": "^3.2.2",
    "vitest": "^3.2.4"
  }
}
