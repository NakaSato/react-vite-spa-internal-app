{
  "name": "react-vite-spa",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "bun run vite --host",
    "build": "bun run vite build",
    "preview": "bun run vite preview",
    "test": "bun test",
    "test:run": "bun run vitest run",
    "test:coverage": "bun run vitest run --coverage",
    "type-check": "bun run tsc --noEmit",
    "lint": "bun run eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 50",
    "lint:fix": "bun run eslint . --ext ts,tsx --fix",
    "lint:strict": "bun run eslint . --ext ts,tsx --max-warnings 0",
    "format": "bun run prettier --write \"src/**/*.{js,ts,jsx,tsx,json,css,md}\"",
    "format:check": "bun run prettier --check \"src/**/*.{js,ts,jsx,tsx,json,css,md}\"",
    "format:all": "bun run prettier --write \"**/*.{js,ts,jsx,tsx,json,css,md}\" --ignore-path .prettierignore",
    "build:analyze": "ANALYZE=true bun run vite build",
    "clean": "rm -rf dist node_modules/.vite bun.lockb",
    "start": "bun run vite --host",
    "install:clean": "rm -rf node_modules bun.lockb && bun install",
    "dev:bun": "bun --hot src/main.tsx"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.6",
    "@mui/icons-material": "^7.2.0",
    "@mui/joy": "^5.0.0-beta.52",
    "@mui/lab": "^7.0.0-beta.14",
    "@mui/material": "^7.2.0",
    "@mui/system": "^7.2.0",
    "@mui/x-charts": "^8.9.2",
    "@mui/x-data-grid": "^8.9.2",
    "@mui/x-date-pickers": "^8.9.2",
    "@mui/x-tree-view": "^8.9.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@react-pdf/renderer": "^4.3.0",
    "@react-spring/web": "^10.0.1",
    "@types/d3": "^7.4.3",
    "@types/socket.io-client": "^3.0.0",
    "buffer": "^6.0.3",
    "chart.js": "^4.5.0",
    "d3": "^7.9.0",
    "framer-motion": "^12.23.12",
    "immer": "^10.1.1",
    "lottie-react": "^2.4.1",
    "pdf-parse": "^1.1.1",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^7.7.1",
    "react-virtualized": "^9.22.6",
    "socket.io-client": "^4.8.1",
    "tesseract.js": "^6.0.1",
    "workbox-core": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0",
    "workbox-webpack-plugin": "^7.3.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.13.5",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vite-plugin-html": "^3.2.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "bun@1.2.18",
  "engines": {
    "bun": ">=1.0.0"
  },
  "trustedDependencies": [
    "tesseract.js"
  ],
  "bun": {
    "install": {
      "frozenLockfile": true,
      "production": false
    }
  }
}
