{
  "name": "react-vite-spa",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:rolldown": "BUILD_TARGET=rolldown vite --config vite.config.rolldown.ts",
    "dev:vercel": "BUILD_TARGET=vercel vite --config vite.config.vercel.ts",
    "build": "NODE_ENV=production vite build",
    "build:analyze": "ANALYZE=true NODE_ENV=production vite build",
    "build:rolldown": "BUILD_TARGET=rolldown NODE_ENV=production vite build --config vite.config.rolldown.ts",
    "build:rolldown:analyze": "BUILD_TARGET=rolldown ANALYZE=true NODE_ENV=production vite build --config vite.config.rolldown.ts",
    "build:vercel": "BUILD_TARGET=vercel NODE_ENV=production vite build --config vite.config.vercel.ts",
    "build:pure-rolldown": "rolldown --config rolldown.config.js",
    "build:optimized": "NODE_ENV=production BUILD_TARGET=optimized vite build",
    "serve": "vite preview",
    "serve:build": "vite preview --port 4173",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "vercel:build": "bun build:vercel",
    "vercel:dev": "bun dev:vercel"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@react-pdf/renderer": "^4.3.0",
    "chart.js": "^4.5.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.7.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "rolldown": "^1.0.0-beta.9-commit.d91dfb5",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "^4.9.5",
    "vite": "^7.0.6",
    "vite-plugin-html": "^3.2.2",
    "vitest": "^3.2.4"
  }
}
